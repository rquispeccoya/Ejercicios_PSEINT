Algoritmo ejer14
	Escribir "INGRESE TAMANIO DEL VECTOR 1"
	Leer t1
	Dimension vector1[t1]
	
	Para i<-1 Hasta  t1 Hacer
		Escribir "Ingrese numero ",i," :  "
		leer n
		vector1[i]=n
	FinPara
	
	Escribir "INGRESE TAMANIO DEL VECTOR 2"
	Leer t2
	Dimension vector2[t2]
	
	Para i<-1 Hasta  t2 Hacer
		Escribir "Ingrese numero ",i," :  "
		leer n
		vector2[i]=n
	FinPara
	t3<-t1+t2
	Dimension  vector3[t3]
	Para i<-1 Hasta  t3 Hacer
		
		vector2[i]=n
	FinPara
FinAlgoritmo
